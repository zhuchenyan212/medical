/* 搜索样式 */

.contA {
  height: 120rpx;
  flex-direction: row;
  padding: 12rpx 12rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
}

.searchcontainer {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  background-color: #f9fafb;
  border-radius: 38rpx;
  height: 76rpx;
  flex-grow: 1;
  box-sizing: border-box;
  padding: 20rpx 20rpx;
}

.letf_search {
  width: 24rpx;
  height: 24rpx;
  flex-shrink: 0;
  margin-right: 20rpx;
}

.input_delete {
  width: 40rpx;
  height: 40rpx;
  flex-shrink: 0;
  margin-left: 20rpx;
}

.search_input {
  font-size: 24rpx;
  color: #333;
  flex-grow: 1;
}

.search_placeholder {
  font-size: 24rpx;
  color: #999;
}

.search_btn {
  line-height: 35px;
  font-size: 28rpx;
  flex-shrink: 0;
  padding: 0rpx;
  text-align: center;
  color: #1a173b;
  font-weight: bold;
  background: none !important;
}

