.Doctoritem{
  display:flex;
  box-sizing:border-box;
  padding:20rpx 24rpx;
  border-bottom:1rpx solid #e2e2e2;
}

.Doctoritem .pro-img image{
    width: 138rpx;
    height: 138rpx;
    border-radius: 50%;
    box-sizing: border-box;
}

.pro-info-box {
    position: relative;
    padding-left: 40rpx;
    padding-top:20rpx;
    line-height: 21px;
    -webkit-box-flex: 1;
    flex: 1;
}

.sexIcon{
