.titleItem {
  width: 650rpx;
  display: flex;
  flex-direction: row;
  /* justify-content: center; */
}

.titleItem2 {
  justify-content: space-between;
}

.titleItem-title {
  font-size: 26rpx;
  color: #333;
  font-weight: bold;
}

.titleInfo {
  font-weight: normal;
  font-size: 24rpx;
}

.titleItem-name {
  font-size: 26rpx;
  color: #489775;
  margin-left: 20rpx;
}

.inputView {
  width: 578rpx;
  display: flex;
  background-color: #f3f3f3;
  padding: 20rpx 36rpx;
  border-radius: 10rpx;
}

.inputV {
  width: 578rpx;
  font-size: 24rpx;
  color: #666;
}

.bottomView-right {
  width: 100%;
  height: 90rpx;
  background-color: #489775;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  font-size: 26rpx;
  color: #fff;
  text-align: center;
  line-height: 90rpx;
  position: fixed;
  left: 0;
  bottom: 0;
}

.bottomView-right image {
  width: 40rpx;
  height: 40rpx;
}

.bottomView-right text {
  font-size: 26rpx;
  color: #fff;
  margin-left: 30rpx;
}
