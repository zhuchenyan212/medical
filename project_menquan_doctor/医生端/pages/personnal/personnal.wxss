/* pages/personnal/personnal.wxss */
page{
  background-color: #F8F8F8;
}
.personnalInfo{
  background-color: #FFFFFF;
  width: 100%;
  height: 190rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  margin-top: 2rpx;
}
.usrIcon{
  width: 130rpx;
  height: 120rpx;
  border-radius: 50%;
}
.userInfo{
  display: flex;
  flex-direction: column;
  margin-left: 48rpx;
}
.userInfo view{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.userInfo-name{
  font-size: 24rpx;
  color: #333333;
}
.userInfo-realName{
  font-size: 20rpx;
  color: #777777
}
.userInfo-tag{
  width: auto;
  height: 28rpx;
  text-align: center;
  line-height: 28rpx;
  border: 2rpx solid #ff0000;
  border-radius: 10rpx;
  font-size: 20rpx;
  color: #ff0000;
  margin-left: 20rpx;
  padding: 0 10rpx;
}
.examSuccess{
border: 2rpx solid #489775;
color: #489775;
}
.QRCode{
  width: 30rpx;
  height: 30rpx;
  margin-left: 186rpx;
}
.showMore{
  width: 100%;
  height: 48rpx;
  background-color: #FFFFFF;
  font-size: 24rpx;
  color: #CACACA;
  margin-top: 2rpx;
  text-align: center;
  line-height: 48rpx;
}
.functionType{
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: center;
  margin-top: 28rpx;
  background-color: #FFFFFF;
  padding-top: 78rpx;
  /* padding-left: 100rpx; */
  padding-left: 0;
}
.btnView{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  /* margin-right: 134rpx; */
  margin-bottom: 80rpx;
  width: 96rpx;
}
.btnView image{
  width: 54rpx;
  height: 54rpx;
}
.btnView text{
  font-size: 24rpx;
  color: #333333;
  margin-top: 5rpx;
}