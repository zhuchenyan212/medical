@import "/pages/component/modal.wxss";

page {
  background: #f9f9f9;
}

.container .keshiItem {
  font-size: 16px;
  height: 90rpx;
  padding-left: 28px;
  background: #fff;
  padding-right: 28px;
  border: 1px solid #f9f9f9;
}

.container .keshiItem .wid {
  background: #fff;
  height: 90rpx;
  line-height: 90rpx;
  text-align: left;
}

.pad {
  padding-left: 10rpx;
  font-size: 10px;
}

.ft12 {
  font-size: 12px;
  line-height: 39px;
  padding-left: 28px;
  color: #8d8d8d;
}

/* 医生列表 */

.Doctoritem {
  display: flex;
  box-sizing: border-box;
  padding: 20rpx 29px;
  margin-bottom: 10rpx;
  background: #fff;
  height: 200rpx;
  overflow:scroll;
}

.Doctoritem .pro-img image {
  width: 116rpx;
  height: 116rpx;
  border-radius: 50%;
}

.pro-info-box {
  position: relative;
  padding-left: 40rpx;
  line-height: 22px;
  -webkit-box-flex: 1;
  flex: 1;
}

.pro-info-box .ft-16 {
  font-size: 16px;
  color: #333;
  font-weight: Medium;
}

.pro-info-box .lable_Doc {
  width: 100%;
  height: 32rpx;
  font-size: 10px;
  border-radius: 8px;
  color: #489775;
  padding-left: 5px;
  padding-right: 5px;
  border: 1px solid #489775;
  margin-left: 10px;
}

.pro-info-box .ft-12 {
  font-size: 12px;
  color: #989898;
}

.pro-info-box .money {
  color: #373737;
  font-size: 12px;
}

.pro-info-box .service {
  color: #989898;
  font-size: 10px;
}

.lable_hos {
  font-size: 12px;
  color: #489775;
  padding-right: 5px;
}
